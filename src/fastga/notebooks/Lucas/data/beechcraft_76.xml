			<aircraft>
				<MTOW units="kg">1770</MTOW>
			</aircraft>
			<propulsion>
				<tank>
					<mass units="kg">20</mass>
				</tank>
			</propulsion>
		</weight>
		<loads>
			<max_shear>
				<cg_position units="m">2.806</cg_position>
			</max_shear>
			<max_rbm>
				<cg_position units="m">2.806</cg_position>
			</max_rbm>
		</loads>
	</data>
	<settings>
		<geometry>
			<fuselage>
				<min_skin_thickness units="m">0.0005</min_skin_thickness>
			</fuselage>
			<fuel_tanks>
				<depth>0.5<!--Average tank depth to the max wing profile depth. The value depends on the shape of the section profile and the allowance made for structure. Typical values lie between 0.5 and 0.8--></depth>
			</fuel_tanks>
		</geometry>
		<weight>
			<airframe>
				<landing_gear>
					<front>
						<weight_ratio>0.3</weight_ratio>
					</front>
				</landing_gear>
			</airframe>
			<aircraft>
				<payload>
					<max_mass_per_passenger units="kg">90</max_mass_per_passenger>
					<design_mass_per_passenger units="kg">80</design_mass_per_passenger>
