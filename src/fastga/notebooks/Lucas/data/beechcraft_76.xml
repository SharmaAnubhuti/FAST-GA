					</fwd>
					<aft>
						<MAC_position>
							<margin>0.05</margin>
						</MAC_position>
					</aft>
				</CG>
			</aircraft>
		</weight>
		<materials>
			<fuselage>
				<skin>
					<density units="kg/m**3">2700</density>
					<young_modulus units="GPa">69.5</young_modulus>
					<sigma_02 units="MPa">110<!--elasticity limit of the material--></sigma_02>
					<sigma_max units="MPa">180<!--ultimate tensile strength of the material--></sigma_max>
				</skin>
				<stringer>
					<density units="kg/m**3">2700</density>
					<young_modulus units="GPa">69.5</young_modulus>
				</stringer>
			</fuselage>
		</materials>
	</settings>
</FASTOAD_model>
