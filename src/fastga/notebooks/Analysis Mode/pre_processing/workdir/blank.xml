<FASTOAD_model>
  <data>
    <TLAR>
      <NPAX_design is_input="True">0.0<!--design number of passengers (w.o. including pilots)--></NPAX_design>
      <luggage_mass_design units="kg" is_input="True">0.0<!--luggage design mass--></luggage_mass_design>
      <range units="nmi" is_input="True">0.0<!--top-level requirement: design range--></range>
      <v_approach units="knot" is_input="True">128.0<!--approach speed--></v_approach>
      <v_cruise units="knot" is_input="True">0.0<!--cruise speed--></v_cruise>
      <v_limit units="knot" is_input="True">0.0<!--limit speed--></v_limit>
      <v_max_sl units="knot" is_input="True">0.0<!--maximum speed at sea level--></v_max_sl>
    </TLAR>
    <Xfoil_pre_processing>
      <reynolds is_input="True">8000000.0</reynolds>
    </Xfoil_pre_processing>
    <geometry>
      <flap_type is_input="True">0.0<!--flap type (0.0 - plain flap, 2.0 - single slotted flap, 3.0 - split flap)--></flap_type>
      <has_T_tail is_input="True">0.0<!--0=horizontal tail is attached to fuselage / 1=horizontal tail is attached to top of vertical tail--></has_T_tail>
      <cabin>
        <aisle_width units="m" is_input="True">0.0<!--width of aisles--></aisle_width>
        <luggage>
          <mass_max units="kg" is_input="True">0.0</mass_max>
        </luggage>
        <seats>
          <passenger>
            <NPAX_max is_input="True">0.0<!--maximum number of passengers in the aircraft--></NPAX_max>
            <count_by_row is_input="True">0.0<!--number of passenger seats per row--></count_by_row>
            <length units="m" is_input="True">0.0<!--passenger seats length--></length>
            <width units="m" is_input="True">0.0<!--width of passenger seats--></width>
          </passenger>
          <pilot>
            <length units="m" is_input="True">0.0<!--pilot seats length--></length>
            <width units="m" is_input="True">0.0<!--width of pilot seats--></width>
          </pilot>
        </seats>
      </cabin>
      <flap>
        <chord_ratio is_input="True">0.0<!--mean value of (flap chord)/(section chord)--></chord_ratio>
        <span_ratio is_input="True">0.0<!--ratio (width of flaps)/(total span)--></span_ratio>
      </flap>
      <fuselage>
        <rear_length units="m" is_input="True">0.0<!--length of rear non-cylindrical part of the fuselage--></rear_length>
      </fuselage>
      <horizontal_tail>
        <area units="m**2" is_input="True">0.0<!--horizontal tail area--></area>
        <elevator_chord_ratio is_input="True">0.0<!--elevator chord ratio--></elevator_chord_ratio>
        <sweep_25 units="deg" is_input="True">0.0<!--sweep angle at 25% chord of horizontal tail--></sweep_25>
        <taper_ratio is_input="True">0.0<!--taper ratio of horizontal tail--></taper_ratio>
        <thickness_ratio is_input="True">0.0<!--thickness ratio of horizontal tail--></thickness_ratio>
        <MAC>
          <at25percent>
            <x>
              <from_wingMAC25 units="m" is_input="True">0.0<!--distance along X between 25% MAC of wing and 25% MAC of horizontal tail--></from_wingMAC25>
            </x>
          </at25percent>
        </MAC>
      </horizontal_tail>
      <landing_gear>
        <type is_input="True">0.0<!--0=non-retractable / 1=retractable--></type>
      </landing_gear>
      <propulsion>
        <count is_input="True">1.0<!--number of engine--></count>
        <layout is_input="True">3.0<!--position of engines (1=under the wing / 2=rear fuselage)--></layout>
        <y_ratio is_input="True">0.0<!--engine position along wing span (if layout=1-2)--></y_ratio>
      </propulsion>
      <vertical_tail>
        <area units="m**2" is_input="True">0.0<!--vertical tail area--></area>
        <aspect_ratio is_input="True">0.0<!--aspect ratio of vertical tail--></aspect_ratio>
        <sweep_25 units="deg" is_input="True">0.0<!--sweep angle at 25% chord of vertical tail--></sweep_25>
        <taper_ratio is_input="True">0.0<!--taper ratio of vertical tail--></taper_ratio>
        <thickness_ratio is_input="True">0.0<!--thickness ratio of vertical tail--></thickness_ratio>
        <max_thickness>
          <x_ratio is_input="True">0.0<!--position of the point of maximum thickness as a ratio of vertical tail chord--></x_ratio>
        </max_thickness>
      </vertical_tail>
      <wing>
        <area units="m**2" is_input="True">0.0<!--wing reference area--></area>
        <aspect_ratio is_input="True">0.0<!--wing aspect ratio--></aspect_ratio>
        <sweep_25 units="deg" is_input="True">0.0<!--sweep angle at 25% chord of wing--></sweep_25>
        <taper_ratio is_input="True">0.0<!--taper ratio of wing--></taper_ratio>
        <thickness_ratio is_input="True">0.0<!--mean thickness ratio of wing--></thickness_ratio>
        <kink>
          <span_ratio is_input="True">0.0<!--ratio (Y-position of kink)/(semi-span)--></span_ratio>
        </kink>
        <MAC>
          <at25percent>
            <x units="m" is_input="True">0.0<!--X-position of the 25% of mean aerodynamic chord of wing w.r.t. aircraft nose (drives position of wing along fuselage)--></x>
          </at25percent>
        </MAC>
      </wing>
    </geometry>
    <aerodynamics>
      <horizontal_tail>
        <efficiency is_input="True">0.0<!--ratio between the dynamic pressure at the tail and the free stream dynamic pressure--></efficiency>
      </horizontal_tail>
    </aerodynamics>
    <handling_qualities>
      <static_margin>
        <target is_input="True">0.0<!--static margin we want to achieve--></target>
      </static_margin>
    </handling_qualities>
    <mission>
      <sizing>
        <fuel units="kg" is_input="True">0.0<!--consumed fuel mass during whole mission--></fuel>
        <cs23>
          <sizing_factor_ultimate is_input="True">0.0<!--ultimate load factor that the aircraft will experience (default value is 5.7)--></sizing_factor_ultimate>
        </cs23>
        <landing>
          <elevator_angle units="deg" is_input="True">0.0<!--position of the elevator during landing--></elevator_angle>
        </landing>
        <takeoff>
          <elevator_angle units="deg" is_input="True">0.0<!--position of the elevator during takeoff--></elevator_angle>
          <flap_angle units="deg" is_input="True">0.0<!--flap angle during takeoff phase in sizing mission--></flap_angle>
          <friction_coefficient_no_brake is_input="True">0.0<!--ground friction coefficient with no brake in sizing mission--></friction_coefficient_no_brake>
          <thrust_rate is_input="True">0.0<!--thrust rate during takeoff phase--></thrust_rate>
        </takeoff>
        <taxi_in>
          <duration units="s" is_input="True">0.0<!--duration of taxi-in phase in sizing mission--></duration>
          <speed units="knot" is_input="True">0.0<!--speed during taxi-in phase in sizing mission--></speed>
          <thrust_rate is_input="True">0.0<!--thrust rate (between 0.0 and 1.0) during taxi-in phase in sizing mission--></thrust_rate>
        </taxi_in>
        <taxi_out>
          <duration units="s" is_input="True">0.0<!--duration of taxi-out phase in sizing mission--></duration>
          <speed units="knot" is_input="True">0.0<!--speed during taxi-out phase in sizing mission--></speed>
          <thrust_rate is_input="True">0.0<!--thrust rate (between 0.0 and 1.0) during taxi-out phase in sizing mission--></thrust_rate>
        </taxi_out>
        <main_route>
          <climb>
            <thrust_rate is_input="True">0.0</thrust_rate>
          </climb>
          <cruise>
            <altitude units="ft" is_input="True">0.0<!--altitude during cruise phase in sizing mission--></altitude>
          </cruise>
          <descent>
            <descent_rate is_input="True">0.0<!--target descent rate for the aircraft--></descent_rate>
          </descent>
          <reserve>
            <duration units="min" is_input="True">0.0<!--duration of the reserve segment--></duration>
          </reserve>
        </main_route>
      </sizing>
    </mission>
    <propulsion>
      <IC_engine>
        <fuel_type is_input="True">1.0<!--engine fuel type (1.0 - gasoline, 2.0 - gasoil)--></fuel_type>
        <max_power units="W" is_input="True">50000.0<!--maximum power of the engine--></max_power>
        <strokes_nb is_input="True">4.0<!--number of strokes on the engine--></strokes_nb>
      </IC_engine>
    </propulsion>
  </data>
  <settings>
    <weight>
      <aircraft>
        <CG>
          <range is_input="True">0.0<!--distance between front position and aft position of CG, as ratio of mean aerodynamic chord (allows to have front position of CG, as currently, FAST-OAD estimates only the aft position of CG)--></range>
        </CG>
      </aircraft>
    </weight>
  </settings>
</FASTOAD_model>
